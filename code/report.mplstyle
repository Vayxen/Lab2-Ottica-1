# Show minor ticks
ytick.minor.visible: True
xtick.minor.visible: True
# Use LaTeX to write all text
text.usetex: True
font.family: serif
# Use 10pt font in plots to match 10pt font in document
axes.titlesize: 11
axes.labelsize: 11
font.size: 11
# Make the legend/label fonts a little smaller
legend.fontsize: 9
xtick.labelsize: 9
ytick.labelsize: 9
# High resolution .png
savefig.dpi: 600
savefig.bbox: tight
# Default image size
figure.figsize: 6.48955306, 4.01076436